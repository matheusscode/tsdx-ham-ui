    "rollup-plugin-replace": "^2.2.0",
    "size-limit": "^8.2.6",
    "style-loader": "^3.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  }
